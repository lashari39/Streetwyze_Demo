<% if @map_asset.errors.any? %>
	$(".error_message").html("<%= escape_javascript render('devise/shared/error_messages', resource: @map_asset) %>")
<% else %>
	$('#userRow_<%= @map_asset.id %>').replaceWith("<%= escape_javascript render('map_asset', map_asset: @map_asset) %>")
	$('.close-model').click()
<% end %>
